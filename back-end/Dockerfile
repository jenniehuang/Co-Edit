FROM node:16
ENV _DB_CONNECT=${_DB_CONNECT} 
ENV _GOOGLE_CLIENT_ID=${_GOOGLE_CLIENT_ID}
ENV _GOOGLE_CLIENT_SECRET=${_GOOGLE_CLIENT_SECRET}
ENV _PASSPORT_SECRET=${_PASSPORT_SECRET}
ENV _SOCKET_ORIGIN=${_SOCKET_ORIGIN}
ENV _SERVER_URI=${_SERVER_URI}
RUN _mkdir -p /home/app
WORKDIR /home/app
COPY . /home/app
RUN npm i 
CMD ["node", "server.js"]